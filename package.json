    "name": "devops-automation",
    "version": "1.1.0",
    "description": "devops automation project for aws devops environment",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "cdk": "cdk",
        "deploy:dev": "cdk deploy dev-devops-automation",
        "bootstrap": "cdk bootstrap",
        "eslint": "eslint . --ext .ts,.js",
        "build:sns-eventhander": "cd ./src/EventHandler && yarn install",
