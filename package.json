        "b": "npm-run-all build:*",
        "build-prod:sns-eventhander": "cd ./src/EventHandler && yarn install --production",
        "build-prod-fns": "npm-run-all build-prod:*",
        "aws:dev": "npm-run-all build-prod-fns deploy:dev"
    },
